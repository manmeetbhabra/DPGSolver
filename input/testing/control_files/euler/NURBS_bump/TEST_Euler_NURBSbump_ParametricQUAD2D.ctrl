pde_name  euler
pde_spec  steady/NURBS_bump

geom_name n-cube
geom_spec x_lr_internal
geometry_parameters_extension internal_channel_NURBS_P6_Q1_NXi11_NEta2

dimension 2

mesh_generator   n-cube/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        quad
mesh_level       1 4
mesh_path        ../meshes/

# Simulation variables

interp_tp  GL
interp_si  WSH
interp_pyr GLL

basis_geom  bezier
basis_sol   lagrange

geom_representation  isoparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen
nurbs_enhanced_metrics  0

p_ref    2 2

fe_method 1

# Testing variables

ml_range_test 1 4
p_range_test  2 2
